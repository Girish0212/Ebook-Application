@model IEnumerable<Ebook_Application.Models.Book>
@{
    ViewBag.Title = "CategoryBook";
}

<h2>Results for "@ViewBag.Search" </h2>
<div class="container-fluid">
    <div class="container">
        @foreach (var item in Model)
        {
            @Html.Partial("BookCard", item)
        }
    </div>

</div>
